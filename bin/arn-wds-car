#!/bin/sh

arn_wds_car() {
    echo "set mode to car"
    uci set wireless.@wifi-iface[0].mode='ap'
    uci set wireless.@wifi-iface[0].ssid='GWS5000_BJWDS'
    uci set wireless.@wifi-iface[0].wds=1
    uci del wireless.@wifi-iface[0].mesh_id 2>/dev/null
    uci commit wireless
    wifi
}

arn_wds_car
