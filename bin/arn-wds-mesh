#!/bin/sh

arn_wds_mesh() {
    echo "set mode to mesh"
    uci set wireless.@wifi-iface[0].mode='mesh'
    uci set wireless.@wifi-iface[0].mesh_id='GWS5000_BJMesh'
    uci set wireless.@wifi-iface[0].wds=1
    uci del wireless.@wifi-iface[0].ssid 2>/dev/null
    uci commit wireless
    wifi
}

arn_wds_mesh